<?xml version="1.0" encoding="utf-8"?>


<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:fitsSystemWindows="true"
	android:background="#D1E5F4"
	android:orientation="vertical">
	
	<androidx.core.widget.NestedScrollView
		android:id="@+id/nestedScrollView"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		app:layout_behavior="com.google.android.material.appbar.AppBarLayout$ScrollingViewBehavior">
		
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical">
			
			<com.google.android.material.card.MaterialCardView
				android:id="@+id/imageCardView"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_margin="8dp"
				
				android:alpha="1.0">
				<LinearLayout
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:orientation="vertical">
					<com.google.android.material.imageview.ShapeableImageView
						android:layout_width="match_parent"
						android:id="@+id/imageView"
						android:layout_height="50dp"
						android:adjustViewBounds="true"
						android:alpha="1.0"
						android:scaleType="centerInside"
						android:layout_marginBottom="5dp"
						android:contentDescription="@string/content_description_media"
						app:srcCompat="@drawable/untitled_design_removebg_preview" />
					<com.google.android.material.imageview.ShapeableImageView
						android:layout_width="match_parent"
						android:id="@+id/imageView2"
						android:layout_height="150dp"
						android:adjustViewBounds="true"
						android:alpha="1.0"
						android:scaleType="centerInside"
						android:contentDescription="@string/content_description_media"
						android:layout_marginBottom="10dp"
						app:srcCompat="@drawable/black_lines_barcode_removebg_preview" />
				</LinearLayout>
			</com.google.android.material.card.MaterialCardView>
			
			
			
			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="vertical"
				android:layout_marginBottom="30dp"
				android:layout_marginTop="10dp"
				>
				
				<com.google.android.material.divider.MaterialDivider
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_margin="10dp"
					android:orientation="horizontal">
				
				
				</com.google.android.material.divider.MaterialDivider>
			
			
			</LinearLayout>
			<TableLayout
				android:id="@+id/grid_layout"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_gravity="center"
				android:gravity="center"
				android:layout_margin="8dp"
				
				android:measureWithLargestChild="true"
				android:stretchColumns="0,1,2">
				
				<TableRow
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					>
					
					<com.google.android.material.card.MaterialCardView
						android:id="@+id/card1"
						style="?attr/materialCardViewOutlinedStyle"
						android:layout_width="0dp"
						android:layout_height="match_parent"
						android:layout_margin="8dp"
						android:clickable="true"
						android:layout_weight="1"
						android:focusable="true">
						
						<LinearLayout
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:orientation="vertical"
							android:layout_margin="10dp">
							
							<!-- Media -->
							<ImageView
								android:layout_width="match_parent"
								android:layout_height="28dp"
								android:contentDescription="@string/content_description_media"
								app:srcCompat="@drawable/baseline_person_outline_24" />
							
							
							<!-- Title -->
							<TextView
								android:paddingTop="15dp"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:layout_gravity="center"
								android:gravity="center"
								android:text="@string/profile"
								android:textColor="@color/black"
								android:textSize="13sp" />
						
						
						</LinearLayout>
					
					</com.google.android.material.card.MaterialCardView>
					
					<com.google.android.material.card.MaterialCardView
						android:id="@+id/card2"
						style="?attr/materialCardViewOutlinedStyle"
						android:layout_width="0dp"
						android:layout_height="match_parent"
						android:layout_margin="8dp"
						android:clickable="true"
						android:layout_weight="1"
						android:focusable="true">
						
						<LinearLayout
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:orientation="vertical"
							android:layout_margin="10dp">
							
							<!-- Media -->
							<ImageView
								android:layout_width="match_parent"
								android:layout_height="28dp"
								
								android:contentDescription="@string/content_description_media"
								app:srcCompat="@drawable/outline_assignment_24" />
							
							
							<!-- Title -->
							<TextView
								android:paddingTop="15dp"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:layout_gravity="center"
								android:gravity="center"
								android:text="@string/order_history"
								android:textColor="@color/black"
								android:textSize="13sp" />
						
						
						</LinearLayout>
					
					</com.google.android.material.card.MaterialCardView>
					
					<com.google.android.material.card.MaterialCardView
						android:id="@+id/card3"
						style="?attr/materialCardViewOutlinedStyle"
						android:layout_width="0dp"
						android:layout_height="match_parent"
						android:layout_margin="8dp"
						android:clickable="true"
						android:layout_weight="1"
						android:focusable="true">
						
						<LinearLayout
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:orientation="vertical"
							android:layout_margin="10dp">
							
							<!-- Media -->
							<ImageView
								android:layout_width="match_parent"
								android:layout_height="28dp"
								
								android:contentDescription="@string/content_description_media"
								app:srcCompat="@drawable/outline_local_mall_24" />
							
							
							<!-- Title -->
							<TextView
								android:paddingTop="15dp"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:layout_gravity="center"
								android:gravity="center"
								android:text="@string/purchase_history"
								android:textColor="@color/black"
								android:textSize="13sp" />
						
						
						</LinearLayout>
					
					</com.google.android.material.card.MaterialCardView>
				</TableRow>
				
				<TableRow
					android:layout_width="0dp"
					android:layout_height="wrap_content">
					
					<com.google.android.material.card.MaterialCardView
						android:id="@+id/card4"
						style="?attr/materialCardViewOutlinedStyle"
						android:layout_width="0dp"
						android:layout_height="match_parent"
						android:layout_margin="8dp"
						android:clickable="true"
						android:layout_weight="1"
						android:focusable="true">
						
						<LinearLayout
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:orientation="vertical"
							android:layout_margin="10dp">
							
							<!-- Media -->
							<ImageView
								android:layout_width="match_parent"
								android:layout_height="28dp"
								
								android:contentDescription="@string/content_description_media"
								app:srcCompat="@drawable/baseline_giftcard_24" />
							
							
							<!-- Title -->
							<TextView
								android:paddingTop="15dp"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:layout_gravity="center"
								android:gravity="center"
								android:text="@string/coupons"
								android:textColor="@color/black"
								android:textSize="13sp" />
						
						
						</LinearLayout>
					
					</com.google.android.material.card.MaterialCardView>
					
					<com.google.android.material.card.MaterialCardView
						android:id="@+id/card5"
						style="?attr/materialCardViewOutlinedStyle"
						android:layout_width="0dp"
						android:layout_height="match_parent"
						android:layout_margin="8dp"
						android:clickable="true"
						android:layout_weight="1"
						android:focusable="true">
						
						<LinearLayout
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:orientation="vertical"
							android:layout_margin="10dp">
							
							<!-- Media -->
							<ImageView
								android:layout_width="match_parent"
								android:layout_height="28dp"
								
								android:contentDescription="@string/content_description_media"
								app:srcCompat="@drawable/outline_rate_review_24" />
							
							
							<!-- Title -->
							<TextView
								android:paddingTop="15dp"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:layout_gravity="center"
								android:gravity="center"
								android:text="@string/store_service_nsurvey_nquestionnare"
								android:textColor="@color/black"
								android:textSize="13sp" />
						
						
						</LinearLayout>
					
					</com.google.android.material.card.MaterialCardView>
					
					<com.google.android.material.card.MaterialCardView
						android:id="@+id/card6"
						style="?attr/materialCardViewOutlinedStyle"
						android:layout_width="0dp"
						android:layout_height="match_parent"
						android:layout_margin="8dp"
						android:clickable="true"
						android:layout_weight="1"
						android:focusable="true">
						
						<LinearLayout
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:orientation="vertical"
							android:layout_margin="10dp">
							
							<!-- Media -->
							<ImageView
								
								android:layout_width="match_parent"
								android:layout_height="28dp"
								
								android:contentDescription="@string/content_description_media"
								app:srcCompat="@drawable/outline_settings_24" />
							
							
							<!-- Title -->
							<TextView
								android:paddingTop="15dp"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:layout_gravity="center"
								android:gravity="center"
								android:text="@string/settings"
								android:textColor="@color/black"
								android:textSize="13sp" />
						
						
						</LinearLayout>
					
					</com.google.android.material.card.MaterialCardView>
				</TableRow>
			</TableLayout>
			
			
			<androidx.recyclerview.widget.RecyclerView
				android:id="@+id/recyclerView"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:orientation="vertical">
			
			
			</androidx.recyclerview.widget.RecyclerView>
			
			<TextView
				android:id="@+id/txt2"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:layout_gravity="center"
				android:gravity="center"
				android:text="@string/version"
				android:paddingTop="20dp"
				android:paddingBottom="100dp">
			
			</TextView>
		
		</LinearLayout>
	
	</androidx.core.widget.NestedScrollView>
</androidx.coordinatorlayout.widget.CoordinatorLayout>
